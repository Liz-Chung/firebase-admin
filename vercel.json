{
  "version": 2,
  "builds": [
    {
      "src": "api/registerUser.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/firestoreHandler.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/registerUser",
      "dest": "/api/registerUser.js"
    },
    {
      "src": "/api/firestoreHandler",
      "dest": "/api/firestoreHandler.js"
    }
  ]
}
